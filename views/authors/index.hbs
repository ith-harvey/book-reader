
{{#each author}}
  <div class="row">
    <div data-id="{{this.id}}" class="panel panel-default col-xs-12">
      <div class="panel-body">
        <div class="col-xs-3">
          <img src="{{this.portrait_url}}"></img>
        </div>
        <div class="blog-title col-xs-9">
          <a href="/authors/{{this.id}}"><h3>{{this.first_name}}{{this.last_name}}</h3></a>
          <div>
          <p>{{this.biography}}</p>
          </div>
        </div>
      </div>
    </div>
  </div>
{{/each}}
